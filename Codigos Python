native=1
data=100
if native == data:
  print("la native y el data son lo mismo")
else:
  print("este native y el data son diferentes")


native=input("ingrese un valor: ")
data=input("ingrese un segundo valor: ")
if native == data:
  print("los valores son similares")
elif native <= data:
  print("native es menor a data")
elif native >= data:
  print("data es mayor a native")
else:
  print("los valores estan mal ingresados")


